{
    "schemaVersion": 1,
    "id": "refinedstorage2",
    "version": "${version}",
    "name": "Refined Storage 2",
    "description": "Refined Storage is a mass storage mod for Minecraft that offers the player a network-based storage system, allowing them to store items and fluids on a massively expandable device network.",
    "authors": [
        "Refined Mods"
    ],
    "contact": {
        "homepage": "https://refinedmods.com/refined-storage",
        "sources": "https://github.com/refinedmods/refinedstorage2",
        "issues": "https://github.com/refinedmods/refinedstorage2/issues"
    },
    "icon": "assets/refinedstorage2/icon.png",
    "license": "MIT",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.refinedmods.refinedstorage2.fabric.Rs2Mod"
        ],
        "client": [
            "com.refinedmods.refinedstorage2.fabric.Rs2ClientMod"
        ],
        "modmenu": [
            "com.refinedmods.refinedstorage2.fabric.integration.ModMenuIntegration"
        ]
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discordapp.com/invite/VYzsydb"
            }
        }
    },
    "mixins": [
        "refinedstorage2.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric": "*",
        "minecraft": "1.16.x",
        "libblockattributes": ">=0.8.4",
        "autoconfig1u": "*",
        "team_reborn_energy": "*"
    },
    "suggests": {
        "patchouli": "*",
        "roughlyenoughitems": "*",
        "modmenu": "*",
        "cloth-config2": "*"
    }
}
